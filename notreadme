#include ,Adafruit_NeoPixel.h>
#ifdef _AVR_
#include <avr/power.h>
#endif

intconst NUM_PIXELS = 12;
int8_t const PIN = 13;
Adafruit_NeoPixel strip;

void setup()
{
  strip = Adafruit_NeoPixel(NUM_PIXELS, PIN, NEO_GRB+NEO_KHZ800);
  strip.begin();
  strip.show();
}

void loop() {
strip.setPixelColor( 0,0xFF0000);
strip.show();
}
